{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowJs": true,
        "strict": false,
        "module": "commonjs",
        "target": "es6",
        "lib": ["es2019", "DOM", "ES6", "DOM.Iterable", "ScriptHost"],
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/*", "src/*"]
        },
        "resolveJsonModule": true
    }
}
