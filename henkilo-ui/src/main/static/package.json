{
    "name": "henkilo-ui",
    "version": "0.1.0",
    "homepage": "/henkilo-ui",
    "dependencies": {
        "@hookform/resolvers": "^2.5.2",
        "@types/node": "14.14.13",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-virtualized-select": "3.0.12",
        "classnames": "2.2.5",
        "history": "3.3.0",
        "isomorphic-fetch": "3.0.0",
        "joi": "^17.4.0",
        "moment": "^2.29.4",
        "normalizr": "3.0.0",
        "oph-urls-js": "github:Opetushallitus/oph-urls-js#master",
        "oph-virkailija-style-guide": "github:Opetushallitus/virkailija-styles#master",
        "papaparse": "5.3.1",
        "ramda": "^0.28.0",
        "react": "16.14.0",
        "react-columns": "1.2.0",
        "react-datepicker": "^4.10.0",
        "react-dom": "16.14.0",
        "react-hook-form": "7.8.4",
        "react-load-script": "0.0.6",
        "react-markdown": "6.0.1",
        "react-onclickoutside": "6.7.0",
        "react-redux": "7.2.6",
        "react-router": "3.2.6",
        "react-router-redux": "4.0.8",
        "react-scroll-to-component": "1.0.2",
        "react-select-fast-filter-options": "0.2.3",
        "react-table": "6.7.4",
        "react-timeout": "1.0.1",
        "react-virtualized": "9.22.3",
        "react-virtualized-select": "3.1.3",
        "react-visibility-sensor": "3.11.0",
        "redux": "4.1.2",
        "redux-thunk": "2.4.1",
        "universal-cookie": "4.0.2"
    },
    "devDependencies": {
        "@playwright/test": "1.29.2",
        "@types/enzyme": "3.10.11",
        "@types/jest": "26.0.19",
        "@types/ramda": "0.28.23",
        "@types/react-datepicker": "^4.8.0",
        "concurrently": "6.1.0",
        "customize-cra": "1.0.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint-config-prettier": "6.11.0",
        "eslint-config-react-app": "7.0.1",
        "jest-enzyme": "^7.1.2",
        "jest-react-hooks-shallow": "1.5.1",
        "nyc": "15.1.0",
        "playwright-test-coverage": "1.2.8",
        "prettier": "2.2.1",
        "react-app-rewired": "2.2.1",
        "react-scripts": "^5.0.1",
        "request": "^2.88.0",
        "shallow-equal-fuzzy": "0.0.2",
        "typescript": "^4.9.5",
        "wait-port": "1.0.4"
    },
    "scripts": {
        "dev": "concurrently --kill-others 'npm start' 'npm run nginx'",
        "nginx": "docker-compose -f ../../../nginx/docker-compose.yml up",
        "start": "BROWSER=none react-scripts start",
        "build": "GENERATE_SOURCEMAP=false react-scripts build",
        "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
        "lint": "eslint --ext .ts,.tsx src",
        "lint:fix": "eslint --ext .ts,.tsx src --fix",
        "prettier": "prettier --check 'src/**/*{ts,tsx}'",
        "prettier:fix": "prettier --write 'src/**/*{ts,tsx}'",
        "eject": "react-scripts eject",
        "test:debug": "react-scripts --inspect-brk test --runInBand",
        "e2e:install-deps": "playwright install chromium && npm --prefix mock-api ci",
        "e2e:mock-api": "npm --prefix mock-api run mock-api",
        "e2e:app": "BROWSER=none react-app-rewired start",
        "e2e:serve": "concurrently -k 'nyc --no-clean --reporter=lcov npm run e2e:app' 'npm:e2e:mock-api'",
        "e2e:test": "wait-port -t 120000 http://:3000/henkilo-ui && playwright test",
        "e2e:ci": "concurrently -s first -k 'npm:e2e:serve' 'npm:e2e:test'"
    },
    "proxy": "http://localhost:8080",
    "browserslist": [
        "last 1 version",
        "> 1%",
        "not dead"
    ],
    "jest": {
        "coverageReporters": [
            "text-summary",
            "html",
            "lcov"
        ]
    },
    "overrides": {
        "eslint-plugin-react": "7.32.2"
    },
    "engines": {
        "npm": "^9",
        "node": "^18"
    }
}
