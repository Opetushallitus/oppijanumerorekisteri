<config xmlns='http://www.ehcache.org/v3'>
    <cache alias="koodistot">
        <expiry>
            <ttl unit="seconds">300</ttl>
        </expiry>
        <resources>
            <heap unit="entries">10</heap>
        </resources>
    </cache>
    <cache alias="oauth2Bearer">
        <expiry>
            <ttl unit="seconds">250</ttl>
        </expiry>
        <resources>
            <heap unit="entries">1</heap>
        </resources>
    </cache>
</config>
